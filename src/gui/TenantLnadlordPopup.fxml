            <padding>
                <Insets bottom="6.0" left="6.0" right="6.0" top="6.0" />
            </padding>
        </VBox>
    </center>
    <bottom>
        <Label alignment="BOTTOM_LEFT" text="Created by DaBase" BorderPane.alignment="BOTTOM_LEFT" />
    </bottom>
